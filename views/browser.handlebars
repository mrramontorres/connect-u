      <td>{{this.dataValues.vcWebsite}}</td>
      <td>{{this.dataValues.vcCity}}</td>
      <td>{{this.dataValues.vcState}}</td>
      <td>{{this.dataValues.vcIndustries}}</td>
      <td>{{this.dataValues.vcState}}</td>
      <td>{{this.dataValues.vcLeadc}}</td>
