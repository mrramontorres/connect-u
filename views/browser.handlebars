    {{#each vcs}}
        <div class="cell medium-4">
      <div class="card">
        <div class="card-section">
          <a href= "{{this.dataValues.vcWebsite}}"><img src="assets/img/{{this.dataValues.vcLogo}}" alt="logo" width="600" height="440"></a> 
          <p>{{this.dataValues.vcName}}</p>
          <a href= "{{this.dataValues.vcWebsite}}"><p>{{this.dataValues.vcWebsite}}</p></a>
          <p>{{this.dataValues.vcCity}}</p>
          <p>{{this.dataValues.vcState}}</p>
          <p>{{this.dataValues.vcIndustries}}</p>
